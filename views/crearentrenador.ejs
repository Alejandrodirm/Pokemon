<%- include("templates/cabecera", {cabecera: 'Nuevo Entrenador' }) %>

    <div class="container">
        <h1>Añadir nuevo entrenador</h1>
        <form action="/entrenador" class="my-2" method="POST">
            <input type="text" placeholder="Nombre del Entrenador" class="form-control my-2" name="nombre">
            <input type="text" placeholder="Ciudad" class="form-control my-2" name="ciudad">
            <input type="text" placeholder="Región" class="form-control my-2" name="region">
            <button type="submit" class="btn btn-dark btn-block">
                Agregar
            </button>
        </form>
    </div>
    <%- include("templates/footer") %>